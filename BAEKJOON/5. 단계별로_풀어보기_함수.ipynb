        "# 4673\n",
        "# 10000보다 작거나 같은 셀프 넘버를 한 줄에 하나씩 출력하는 프로그램을 작성하시오.\n",
        "\n",
        "def self_num(num) :\n",
        "\n",
        "  for i in list(str(num)) :\n",
        "    num += int(i)\n",
        "\n",
        "  return num\n",
        "\n",
        "num_list = []\n",
        "\n",
        "for i in range(10000) :\n",
        "  num_list.append(self_num(i))\n",
        "\n",
        "for o in range(10000) :\n",
        "  if o in num_list :\n",
        "    pass\n",
        "\n",
        "  else :\n",
        "    print(o)\n",
        "\n",
        "self_num(10000)"
